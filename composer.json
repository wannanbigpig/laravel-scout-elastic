{
  "name": "wannanbigpig/laravel-scout-elastic",
  "description": "Elastic Driver for Laravel Scout",
  "license": "MIT",
  "authors": [
    {
      "name": "wannanbigpig",
      "email": "liuml0211@gmail.com"
    }
  ],
  "keywords": [
    "laravel",
    "scout",
    "elasticsearch",
    "elastic"
  ],
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "laravel/scout": "^9.3|^10.0",
    "elasticsearch/elasticsearch": "^7.9"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^8.0|^9.3"
  },
  "suggest": {
    "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^7.9)."
  },
  "autoload": {
    "psr-4": {
      "Wannanbigpig\\LaravelScoutElastic\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wannanbigpig\\LaravelScoutElastic\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wannanbigpig\\LaravelScoutElastic\\LaravelScoutElasticProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
